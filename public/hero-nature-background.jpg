<!-- Colombian Nature Landscape - AI Generated Style -->
<svg width="1920" height="1080" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Sky gradient with Colombian sunset colors -->
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#98D8E8;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#F4A261;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E76F51;stop-opacity:0.8" />
    </linearGradient>

    <!-- Mountain gradients -->
    <linearGradient id="mountainGradient1" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#8B7355;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#654321;stop-opacity:1" />
    </linearGradient>

    <linearGradient id="mountainGradient2" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#A0522D;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B4513;stop-opacity:1" />
    </linearGradient>

    <!-- Tree gradients -->
    <radialGradient id="treeGradient" cx="50%" cy="30%" r="50%">
      <stop offset="0%" style="stop-color:#228B22;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#32CD32;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#006400;stop-opacity:1" />
    </radialGradient>

    <!-- Cloud filter for soft effect -->
    <filter id="cloudFilter" x="0%" y="0%" width="200%" height="200%">
      <feGaussianBlur in="SourceGraphic" stdDeviation="3"/>
    </filter>
  </defs>

  <!-- Sky background -->
  <rect width="1920" height="1080" fill="url(#skyGradient)"/>

  <!-- Distant mountains -->
  <path d="M0 400 L300 350 L600 380 L900 340 L1200 370 L1500 330 L1800 360 L1920 350 L1920 600 L0 600 Z"
        fill="url(#mountainGradient1)" opacity="0.8"/>
  <path d="M0 450 L250 400 L500 420 L750 390 L1000 410 L1250 385 L1500 405 L1750 395 L1920 400 L1920 650 L0 650 Z"
        fill="url(#mountainGradient2)" opacity="0.9"/>

  <!-- Closer mountains with texture -->
  <path d="M0 500 L200 450 L400 470 L600 440 L800 460 L1000 430 L1200 450 L1400 425 L1600 445 L1800 435 L1920 450 L1920 700 L0 700 Z"
        fill="url(#mountainGradient1)" opacity="0.95"/>
  <!-- Mountain texture lines -->
  <path d="M0 520 L150 495 L300 505 L450 485 L600 500 L750 480 L900 495 L1050 475 L1200 490 L1350 470 L1500 485 L1650 465 L1800 480 L1920 475"
        stroke="#654321" stroke-width="2" fill="none" opacity="0.6"/>
  <path d="M0 550 L180 525 L360 540 L540 515 L720 535 L900 510 L1080 530 L1260 505 L1440 525 L1620 500 L1800 520 L1920 515"
        stroke="#8B4513" stroke-width="1.5" fill="none" opacity="0.4"/>

  <!-- Trees in foreground -->
  <!-- Large trees -->
  <circle cx="150" cy="650" r="35" fill="url(#treeGradient)"/>
  <circle cx="140" cy="620" r="25" fill="url(#treeGradient)"/>
  <rect x="145" y="685" width="10" height="50" fill="#8B4513"/>
  <rect x="140" y="735" width="20" height="15" fill="#654321"/>

  <circle cx="450" cy="670" r="30" fill="url(#treeGradient)"/>
  <circle cx="435" cy="645" r="22" fill="url(#treeGradient)"/>
  <rect x="440" y="700" width="8" height="45" fill="#8B4513"/>

  <circle cx="750" cy="655" r="40" fill="url(#treeGradient)"/>
  <circle cx="735" cy="625" r="28" fill="url(#treeGradient)"/>
  <circle cx="755" cy="610" r="18" fill="url(#treeGradient)"/>
  <rect x="745" y="695" width="12" height="55" fill="#8B4513"/>
  <rect x="738" y="750" width="24" height="18" fill="#654321"/>

  <!-- Medium trees -->
  <circle cx="1050" cy="680" r="25" fill="url(#treeGradient)"/>
  <circle cx="1040" cy="660" r="18" fill="url(#treeGradient)"/>
  <rect x="1045" y="705" width="6" height="35" fill="#8B4513"/>

  <circle cx="1350" cy="675" r="28" fill="url(#treeGradient)"/>
  <circle cx="1335" cy="650" r="20" fill="url(#treeGradient)"/>
  <rect x="1340" y="703" width="7" height="40" fill="#8B4513"/>

  <!-- Small trees scattered -->
  <circle cx="300" cy="720" r="15" fill="url(#treeGradient)"/>
  <rect x="297" y="735" width="4" height="20" fill="#8B4513"/>

  <circle cx="600" cy="730" r="18" fill="url(#treeGradient)"/>
  <rect x="597" y="748" width="5" height="25" fill="#8B4513"/>

  <circle cx="900" cy="715" r="16" fill="url(#treeGradient)"/>
  <rect x="897" y="731" width="4" height="22" fill="#8B4513"/>

  <circle cx="1200" cy="725" r="20" fill="url(#treeGradient)"/>
  <rect x="1197" y="745" width="5" height="28" fill="#8B4513"/>

  <circle cx="1500" cy="710" r="17" fill="url(#treeGradient)"/>
  <rect x="1497" y="727" width="4" height="24" fill="#8B4513"/>

  <circle cx="1700" cy="735" r="19" fill="url(#treeGradient)"/>
  <rect x="1697" y="754" width="5" height="26" fill="#8B4513"/>

  <!-- Clouds -->
  <ellipse cx="300" cy="150" rx="80" ry="25" fill="#FFFFFF" opacity="0.8" filter="url(#cloudFilter)"/>
  <ellipse cx="320" cy="140" rx="60" ry="20" fill="#FFFFFF" opacity="0.9" filter="url(#cloudFilter)"/>

  <ellipse cx="800" cy="120" rx="90" ry="30" fill="#FFFFFF" opacity="0.7" filter="url(#cloudFilter)"/>
  <ellipse cx="820" cy="110" rx="70" ry="25" fill="#FFFFFF" opacity="0.8" filter="url(#cloudFilter)"/>
  <ellipse cx="840" cy="125" rx="50" ry="18" fill="#FFFFFF" opacity="0.9" filter="url(#cloudFilter)"/>

  <ellipse cx="1400" cy="160" rx="75" ry="28" fill="#FFFFFF" opacity="0.75" filter="url(#cloudFilter)"/>
  <ellipse cx="1420" cy="150" rx="55" ry="22" fill="#FFFFFF" opacity="0.85" filter="url(#cloudFilter)"/>

  <!-- Grass/details in foreground -->
  <path d="M0 780 Q100 770 200 780 Q300 775 400 780 Q500 785 600 780 Q700 775 800 780 Q900 785 1000 780 Q1100 775 1200 780 Q1300 785 1400 780 Q1500 775 1600 780 Q1700 785 1800 780 Q1900 775 1920 780 L1920 1080 L0 1080 Z"
        fill="#228B22" opacity="0.6"/>
  <!-- Grass blades -->
  <path d="M50 780 L45 760 M70 785 L65 765 M90 780 L85 760 M110 785 L105 765 M130 780 L125 760"
        stroke="#32CD32" stroke-width="1" fill="none" opacity="0.8"/>
  <path d="M250 780 L245 760 M270 785 L265 765 M290 780 L285 760 M310 785 L305 765 M330 780 L325 760"
        stroke="#32CD32" stroke-width="1" fill="none" opacity="0.8"/>
  <path d="M450 780 L445 760 M470 785 L465 765 M490 780 L485 760 M510 785 L505 765 M530 780 L525 760"
        stroke="#32CD32" stroke-width="1" fill="none" opacity="0.8"/>

  <!-- Sun glow -->
  <circle cx="1600" cy="200" r="60" fill="#FFD700" opacity="0.3"/>
  <circle cx="1600" cy="200" r="40" fill="#FFA500" opacity="0.4"/>
  <circle cx="1600" cy="200" r="20" fill="#FF8C00" opacity="0.6"/>

  <!-- Birds (small details) -->
  <path d="M 500 300 Q 510 295 520 300 Q 510 305 500 300 Z" fill="#000000" opacity="0.6"/>
  <path d="M 550 320 Q 560 315 570 320 Q 560 325 550 320 Z" fill="#000000" opacity="0.6"/>
  <path d="M 1200 280 Q 1210 275 1220 280 Q 1210 285 1200 280 Z" fill="#000000" opacity="0.6"/>
</svg>