<!-- Authentic Colombian Countryside Landscape -->
<svg width="1920" height="1080" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Colombian tropical sky -->
    <linearGradient id="colombianSky" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="35%" style="stop-color:#98D8E8;stop-opacity:1" />
      <stop offset="75%" style="stop-color:#F4A261;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#E9C46A;stop-opacity:0.8" />
    </linearGradient>

    <!-- Mountain ranges -->
    <linearGradient id="andesMountains" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#D2B48C;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#BC8F5E;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B7355;stop-opacity:1" />
    </linearGradient>

    <linearGradient id="closeMountains" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#A0522D;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#654321;stop-opacity:1" />
    </linearGradient>

    <!-- Colombian vegetation -->
    <radialGradient id="tropicalTrees" cx="50%" cy="30%" r="60%">
      <stop offset="0%" style="stop-color:#228B22;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#32CD32;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#006400;stop-opacity:1" />
    </radialGradient>

    <radialGradient id="coffeePlants" cx="50%" cy="40%" r="55%">
      <stop offset="0%" style="stop-color:#228B22;stop-opacity:1" />
      <stop offset="65%" style="stop-color:#32CD32;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#006400;stop-opacity:1" />
    </radialGradient>

    <!-- River water -->
    <linearGradient id="riverWater" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#4682B4;stop-opacity:0.9" />
      <stop offset="50%" style="stop-color:#87CEEB;stop-opacity:0.95" />
      <stop offset="100%" style="stop-color:#4682B4;stop-opacity:0.9" />
    </linearGradient>

    <!-- Cloud filter -->
    <filter id="softCloud" x="0%" y="0%" width="200%" height="200%">
      <feGaussianBlur in="SourceGraphic" stdDeviation="3"/>
      <feMorphology operator="dilate" radius="1.5"/>
    </filter>
